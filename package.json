{
  "name": "orbital-sync",
  "version": "1.5.7",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "install:hooks": "git config core.hooksPath '.hooks'",
    "lint": "eslint . && prettier --ignore-unknown --check .",
    "lint:fix": "eslint . --fix && prettier --ignore-unknown --write .",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules npx jest --silent",
    "test:e2e": "docker compose -f docker-compose.test.yml up --build --abort-on-container-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattwebbio/orbital-sync.git"
  },
  "bin": "./dist/index.js",
  "devDependencies": {
    "@commitlint/config-conventional": "^18.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.4.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.8.0",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitlint": "^18.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.4.1",
    "nock": "^13.5.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@honeybadger-io/js": "^6.1.1",
    "chalk": "^4.1.2",
    "fetch-cookie": "^2.1.0",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.12",
    "nodemailer": "^6.9.9",
    "sleep-promise": "^9.1.0"
  }
}
